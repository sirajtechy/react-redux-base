    "start-dev-debug": "cross-en  BUILDSCRIPT=start-dev-debug OFFLINE=1 concurrently -k -p \"[{name}]\" -n \"Server, React\" \"npm run server-dev\" \"npm run react-debug\"",
    "react-debug": "cross-env PORT=5008 DEBUG=1 webpack-dev-server --mode development --inline --content-base dist --history-api-fallback --port 8089",
    "start-dev": "cross-env BUILDSCRIPT=start-dev concurrently -k -p \"[{name]\" -n \"Server,React\"  \"npm run server-dev\" \"npm run react\"",
    "react": "cross-env PORT=5008 webpack-dev-server --mode development --inline --content-base dist --history-api-fallback --port 8089",
    "server-dev": "cross-env PORT=5008 NODE_ENV=server nodemon --watch server -e js,json,txt,xml  server/server.js --exec babel-node",
    "build": "cross-env NODE_ENV=production BUILDSCRIPT=build webpack --mode production",
    "lint": "eslint src/"
  },
